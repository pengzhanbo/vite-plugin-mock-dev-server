{
  "name": "vite-plugin-mock-dev-server",
  "type": "module",
  "version": "2.0.7",
  "author": "pengzhanbo <q942450674@outlook.com> (https://github.com/pengzhanbo)",
  "license": "MIT",
  "homepage": "https://vite-plugin-mock-dev-server.netlify.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pengzhanbo/vite-plugin-mock-dev-server"
  },
  "keywords": [
    "vite",
    "plugin",
    "vite-plugin",
    "mock",
    "mock-server",
    "development"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./helper": {
      "import": {
        "types": "./dist/helper.d.mts",
        "default": "./dist/helper.mjs"
      }
    },
    "./server": {
      "import": {
        "types": "./dist/server.d.mts",
        "default": "./dist/server.mjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^20 || >=22"
  },
  "scripts": {
    "build": "tsdown"
  },
  "peerDependencies": {
    "esbuild": ">=0.21.0",
    "rolldown": ">=1.0.0-beta.1",
    "vite": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "esbuild": {
      "optional": true
    },
    "rolldown": {
      "optional": true
    }
  },
  "dependencies": {
    "@pengzhanbo/utils": "catalog:prod",
    "ansis": "catalog:prod",
    "chokidar": "catalog:prod",
    "co-body": "catalog:prod",
    "cors": "catalog:prod",
    "debug": "catalog:prod",
    "formidable": "catalog:prod",
    "http-status": "catalog:prod",
    "is-core-module": "catalog:prod",
    "json5": "catalog:prod",
    "local-pkg": "catalog:prod",
    "mime-types": "catalog:prod",
    "path-to-regexp": "catalog:prod",
    "picomatch": "catalog:prod",
    "tinyglobby": "catalog:prod",
    "ws": "catalog:prod"
  },
  "devDependencies": {
    "esbuild": "catalog:dev",
    "rolldown": "catalog:dev",
    "vite": "catalog:dev"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
